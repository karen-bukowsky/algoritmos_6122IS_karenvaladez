Algoritmo numMayornumMenor
	definir n1, n2 como real;
	escribir "introduzca el valor numero 1";
	leer n1;
	escribir "introduzca el valor numero 2";
	leer n2;
	si (n1>n2) entonces 
		escribir "el resultado de la division es de: ", (n1/n2);
		sino 
			si (n2>n1) entonces 
				escribir "el resultado de la division es de: ", (n2/n1);
			
		FinSi
	FinSi
	
FinAlgoritmo
